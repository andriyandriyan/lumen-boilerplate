{
    "name": "lumen-boilerplate",
    "description": "Rest API Boilerplate Using Lumen",
    "keywords": ["lumen", "api"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Andriy",
            "email": "andriy.andriyan1@gmail.com",
            "homepage": "https://andriyandriyan.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-cors": "^0.11.3",
        "laravel/lumen-framework": "5.8.*",
        "tymon/jwt-auth": "^1.0.0-rc.4.1"
    },
    "require-dev": {
    	"flipbox/lumen-generator": "^5.6",
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
